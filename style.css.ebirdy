input[type=text] {
	font-size: 11px;
	
	height: 18px;
	line-height:18px;
	border: 1px solid #3d007b;
	background-color: white;
	padding-left: 6px;
	
	padding-right: 6px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
width:95%;
	behavior: url(/PIE.htc);
}
input[type=text]:disabled {
	font-size: 11px;
	
	height: 18px;
	line-height:18px;
	border: 1px solid #666;
	background-color:#CCC;
	padding-left: 6px;
	padding-right: 6px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
        width:95%;
	behavior: url(/PIE.htc);
	color:#000;
	cursor:not-allowed;
}

input[type=checkbox]:disabled {
	border: 1px solid #666;
	background-color:#CCC;
	color:#000;
	cursor:not-allowed;
}
select {
	
	font-size: 14px;

	height: 22px;
	line-height:18px;
	border: 1px solid #3d007b;
	background-color: white;
	padding-left: 10px;
	padding-right: 10px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
width:100%;
	behavior: url(/PIE.htc);

	line-height:24px;
	color:#3d007b;
}
select:disabled {
	border: 1px solid #737373;
	color:#000;
	background:#CCCCCC;
	cursor: not-allowed;
}
option:hover{
	border-left:8px solid #3d007b;
	background-color:#8EB4E3;
}

